{
  "expo": {
    "scheme": "fortunai",
    "name": "FortunAI",
    "slug": "fortunai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#36010F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fortunai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fortunai.app",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5016aeb9-9591-41f6-89f9-ccd49c488207"
      }
    },
    "owner": "kaungsanthwin"
  }
}
